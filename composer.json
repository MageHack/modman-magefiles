{
    "name": "magehack/modman-magefiles",
    "type": "magento-shell-module",
    "description": "Generates file mappings for a Modman or Composer Magento module",
    "require": {
        "magento-hackathon/magento-composer-installer": "*",
    },
    "extra": {
        "map": [
            [
                "modman/generator",
                "shell/magehack/modman/generator.php"
            ],
            [
                "modman/files",
                "shell/magehack/modman/files.php"
            ],
            [
                "modman/helper",
                "shell/magehack/modman/helper.php"
            ],
            [
                "abstract.php",
                "shell/abstract.php"
            ]
        ]
    }
}
